//dapetin datanya dulu
$query = mysqli_query($connect, "select * from table_lama where id='0'");
$data = mysqli_fetch_array($query);
if ($data!==NULL){
    echo "Tidak ada data";
}
$nama = $data["nama_barang"];
$jumlah = $data["jumlah"];

//masukin datanya lagi
mysqli_query($connect, "insert into table_baru(`nama`, `jumlah`) values("$nama", "$jumlah"));
